  keywords = {BIOREMEDIATION,Ecotoxicology,Environmental Chemistry,Soil Science \& Conservation,Waste Water Technology / Water Pollution Control / Water Management / Aquatic Pollution,Mercury,Biotransformation,Environment; general,Environmental Science and Engineering,Volatilization},
  pages = {995-1002},
  file = {C:\\Users\\asw\\Zotero\\storage\\4R4JRFV9\\Gupta and Nirwan - 2014 - Evaluation of mercury biotransformation by heavy m.pdf;C:\\Users\\asw\\Zotero\\storage\\9TCXAFPK\\10.html}
}

@article{bauvaisSpongingMetalsBacteria2015,
