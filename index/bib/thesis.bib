  author = {Ehrlich, Henry Lutz and Newman, Dianne K.},
  month = dec,
  year = {2008},
  keywords = {Science / Earth Sciences / Geology,Science / Life Sciences / Microbiology,Science / Life Sciences / Molecular Biology}
}

